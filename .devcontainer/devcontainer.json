{
  "name": "PySpark + MySQL",
  "dockerComposeFile": "docker-compose.yml",
  "service": "workspace",
  "workspaceFolder": "/workspaces/pyspark-hands-on",
  "features": {
    "ghcr.io/devcontainers/features/java:latest": {
      "version": "17"
    }
  },
  "postCreateCommand": "pip install pyspark mysql-connector-python",
  "customizations": {
    "vscode": {
      "extensions": ["ms-python.python"]
    }
  }
}
